{
  "configId": 3,
  "configName": "Accredited Enrolment Form - Single - No Payment",
  "step": "schooling",
  "stepName": "Schooling",
  "stepHeader": "Secondary Education",
  "stepType": "contact-update",
  "stepOrder": 9,
  "stepId": "EducationsHistory",
  "contactType": "Student",
  "buttonText": "Save",
  "fields": [
    {
      "order": 1,
      "fieldId": "highestschoollevel",
      "name": "What is your highest completed school level?",
      "type": "select",
      "required": true,
      "dataField": true,
      "syncToCustomField": false,
      "mapsToContactField": "HIGHESTSCHOOLLEVELID",
      "tooltip": "If you are currently enrolled in secondary education, the Highest school level completed refers to the highest school level you have actually completed and not the level you are currently undertaking. For example, if you are currently in Year 10 the Highest school level completed is Year 9.",
      "options": [
        { "display": "Did not attend school", "value": "2" },
        { "display": "Year 8 or Below", "value": "8" },
        { "display": "Year 9", "value": "9" },
        { "display": "Year 10", "value": "10" },
        { "display": "Year 11", "value": "11" },
        { "display": "Year 12", "value": "12" }
      ],
      "events": {
        "hide_high_school_fields": { "action": "hide", "trigger": "change" },
        "show_high_school_fields": { "action": "show", "trigger": "change" },
        "show_high_school_fields10": { "action": "show", "trigger": "change" },
        "show_high_school_fields8": { "action": "show", "trigger": "change" },
        "show_high_school_fields9": { "action": "show", "trigger": "change" }
      }
    },
    {
      "order": 2,
      "fieldId": "yearhighestschoolcompleted",
      "name": "Year Highest School Completed",
      "type": "text",
      "required": false,
      "dataField": true,
      "syncToCustomField": false,
      "mapsToContactField": "HIGHESTSCHOOLLEVELYEAR",
      "tooltip": "The calendar year that the highest level of schooling was completed.",
      "hideInitially": "visible",
      "placeholder": "e.g., 2020"
    },
    {
      "order": 3,
      "fieldId": "stillenrolledsecondary",
      "name": "Are you still enrolled in secondary or senior secondary education?",
      "type": "select",
      "required": false,
      "dataField": true,
      "syncToCustomField": false,
      "mapsToContactField": "ATSCHOOLFLAG",
      "hideInitially": "visible",
      "options": [
        { "display": "Not Specified", "value": "" },
        { "display": "Yes", "value": "true" },
        { "display": "No", "value": "false" }
      ],
      "events": {
        "hide_high_school_fields": { "action": "hide", "trigger": "change" },
        "show_high_school_fields": { "action": "show", "trigger": "change" },
        "show_high_school_fields10": { "action": "show", "trigger": "change" },
        "show_high_school_fields11": { "action": "show", "trigger": "change" },
        "show_high_school_fields8": { "action": "show", "trigger": "change" },
        "show_high_school_fields9": { "action": "show", "trigger": "change" }
      }
    },
    {
      "order": 4,
      "fieldId": "tertiaryEducationDetails",
      "name": "Previous qualifications achieved",
      "type": "information",
      "required": false
    },
    {
      "order": 5,
      "fieldId": "prioreducationstatus",
      "name": "Have you successfully completed any post-secondary education?",
      "type": "select",
      "required": true,
      "dataField": true,
      "syncToCustomField": false,
      "mapsToContactField": "PRIOREDUCATIONSTATUS",
      "options": [
        { "display": "Yes", "value": "true" },
        { "display": "No", "value": "false" }
      ],
      "events": {
        "prioreducation_show": {
          "trigger": "change",
          "action": "show",
          "condition": "value === 'true'"
        },
        "prioreducation_hide": {
          "trigger": "change",
          "action": "hide",
          "condition": "value === 'false'"
        }
      }
    },
    {
      "order": 6,
      "fieldId": "prioreducation",
      "name": "Prior Education",
      "type": "checkbox-with-recognition",
      "required": true,
      "dataField": true,
      "syncToCustomField": false,
      "mapsToContactField": "PRIOREDUCATION",
      "hideInitially": "hidden",
      "tooltip": "Select any relevant prior education achieved.",
      "recognitionOptions": [
        { "display": "Not specified", "value": "" },
        { "display": "Australian Qualification", "value": "AustQual" },
        { "display": "Australian Equivalent", "value": "AustEquiv" },
        { "display": "International", "value": "International" }
      ],
      "options": [
        { "display": "Bachelor Degree or Higher Degree level", "value": "008", "recognitionField": "prioreducation_008_recognition" },
        { "display": "Advanced Diploma or Associate Degree Level", "value": "410", "recognitionField": "prioreducation_410_recognition" },
        { "display": "Diploma Level", "value": "420", "recognitionField": "prioreducation_420_recognition" },
        { "display": "Certificate IV", "value": "511", "recognitionField": "prioreducation_511_recognition" },
        { "display": "Certificate III", "value": "514", "recognitionField": "prioreducation_514_recognition" },
        { "display": "Certificate II", "value": "521", "recognitionField": "prioreducation_521_recognition" },
        { "display": "Certificate I", "value": "524", "recognitionField": "prioreducation_524_recognition" },
        { "display": "Other education (including certificates or overseas qualifications not listed above)", "value": "990", "recognitionField": "prioreducation_990_recognition" }
      ]
    }
  ]
}
